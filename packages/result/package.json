{
  "name": "@hex-di/result",
  "version": "0.1.0",
  "description": "Rust-style Result type for TypeScript - errors as values with full type safety",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./fn": {
      "import": {
        "types": "./dist/fn/index.d.ts",
        "default": "./dist/fn/index.js"
      }
    },
    "./fn/*": {
      "import": {
        "types": "./dist/fn/*.d.ts",
        "default": "./dist/fn/*.js"
      }
    },
    "./async": {
      "import": {
        "types": "./dist/async/result-async.d.ts",
        "default": "./dist/async/result-async.js"
      }
    },
    "./combinators": {
      "import": {
        "types": "./dist/combinators/index.d.ts",
        "default": "./dist/combinators/index.js"
      }
    },
    "./errors": {
      "import": {
        "types": "./dist/errors/index.d.ts",
        "default": "./dist/errors/index.js"
      }
    },
    "./option": {
      "import": {
        "types": "./dist/option/index.d.ts",
        "default": "./dist/option/index.js"
      }
    },
    "./unsafe": {
      "import": {
        "types": "./dist/unsafe/index.d.ts",
        "default": "./dist/unsafe/index.js"
      }
    },
    "./internal/*": null
  },
  "files": [
    "dist",
    "src",
    "!src/**/*.test.ts",
    "!src/**/__fixtures__",
    "!dist/**/*.test.*",
    "!dist/**/__fixtures__",
    "!dist/tsconfig.tsbuildinfo"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "lint": "eslint . --cache",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:mutation": "stryker run",
    "test:types": "vitest run --typecheck",
    "typecheck": "tsc --noEmit",
    "bench": "vitest bench",
    "test:cucumber": "cucumber-js"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^12.6.0",
    "@stryker-mutator/core": "^9.5.1",
    "@stryker-mutator/vitest-runner": "^9.5.1",
    "@vitest/coverage-v8": "^4.0.18",
    "tsx": "^4.21.0"
  },
  "keywords": [
    "result",
    "either",
    "error-handling",
    "typescript",
    "type-safe",
    "functional",
    "monadic"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hex-di/result.git",
    "directory": "packages/result"
  },
  "peerDependencies": {
    "typescript": ">=5.6"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
